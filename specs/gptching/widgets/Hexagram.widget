{
  "version": "1.0",
  "name": "Hexagram",
  "template": "{\"type\":\"Card\",\"size\":\"full\",\"children\":[{\"type\":\"Col\",\"align\":\"center\",\"gap\":3,\"padding\":{\"y\":4},\"children\":[{\"type\":\"Title\",\"value\":{{ (symbol) | tojson }},\"size\":\"5xl\",\"textAlign\":\"center\"},{\"type\":\"Title\",\"value\":{{ ((number ~ \" · \" ~ englishName)) | tojson }},\"size\":\"md\"},{\"type\":\"Row\",\"gap\":2,\"children\":[{\"type\":\"Badge\",\"label\":{{ (chinese) | tojson }},\"color\":\"discovery\"},{\"type\":\"Badge\",\"label\":{{ ((\"Pinyin: \" ~ pinyin)) | tojson }}}]},{\"type\":\"Text\",\"value\":{{ (description) | tojson }},\"color\":\"secondary\",\"textAlign\":\"center\"}]}]}",
  "jsonSchema": {
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "type": "object",
    "properties": {
      "symbol": {
        "type": "string"
      },
      "englishName": {
        "type": "string"
      },
      "number": {
        "type": "string"
      },
      "chinese": {
        "type": "string"
      },
      "pinyin": {
        "type": "string"
      },
      "description": {
        "type": "string"
      }
    },
    "required": [
      "symbol",
      "englishName",
      "number",
      "chinese",
      "pinyin",
      "description"
    ],
    "additionalProperties": false
  },
  "outputJsonPreview": {
    "type": "Card",
    "size": "full",
    "children": [
      {
        "type": "Col",
        "align": "center",
        "gap": 3,
        "padding": {
          "y": 4
        },
        "children": [
          {
            "type": "Title",
            "value": "䷀",
            "size": "5xl",
            "textAlign": "center"
          },
          {
            "type": "Title",
            "value": "1 · The Creative / Force",
            "size": "md"
          },
          {
            "type": "Row",
            "gap": 2,
            "children": [
              {
                "type": "Badge",
                "label": "乾",
                "color": "discovery"
              },
              {
                "type": "Badge",
                "label": "Pinyin: qián"
              }
            ]
          },
          {
            "type": "Text",
            "value": "Pure creative force and initiating power. Leadership, vision, and continuous development.",
            "color": "secondary",
            "textAlign": "center"
          }
        ]
      }
    ]
  },
  "encodedWidget": "-"
}